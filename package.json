{
  "name": "flalxrn",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir dist",
    "build:web:pages": "cross-env PUBLIC_URL=/flalx-react-native/ expo export --platform web --output-dir dist",
    "postbuild:web": "node -e \"require('fs').copyFileSync('dist/index.html','dist/404.html')\"",
    "predeploy": "npm run build:web:pages && npm run postbuild:web",
    "deploy": "gh-pages -d dist -b gh-pages"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-bouncy-checkbox",
          "react-native-responsive-fontsize",
          "base64-arraybuffer"
        ]
      }
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.7",
    "@supabase/supabase-js": "^2.55.0",
    "axios": "^1.12.2",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~53.0.23",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "^18.1.11",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.7",
    "expo-status-bar": "~2.2.3",
    "i18next": "^25.3.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.6",
    "react-native-bouncy-checkbox": "^4.1.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-localize": "^3.5.2",
    "react-native-paper": "^5.14.5",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "react-native-webview": "^13.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "cross-env": "^10.1.0",
    "gh-pages": "^6.3.0"
  },
  "private": true
}
