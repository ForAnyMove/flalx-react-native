{
  "expo": {
    "name": "FlalxRN",
    "slug": "flalx-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/flalx-logo-large.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo/preview-logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": { "NSAllowsArbitraryLoads": true }
      }
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/flalx-logo-large.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alexfam.flalxapp",
    },
    "web": {
      "favicon": "./assets/logo/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1ee54fd1-9a7c-4b7c-9e82-8e5739c78ae6"
      }
    },
    "owner": "alexfam",
    "plugins": [
      "expo-router",
      "expo-localization"
    ]
  }
}
